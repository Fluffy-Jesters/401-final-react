/store/store.js",[],"/home/brend/fluffy-jesters/401-final-react/src/App.js",["116"],"import React from 'react';\nimport {useSelector, Provider} from 'react-redux';\nimport {BrowserRouter, Switch, Route} from 'react-router-dom';\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport './app.scss'\nimport Landing from './components/landing-page/landingPage.js';\nimport Game from './components/player-hub/game-table/gameTable.js';\nimport About from './components/aboutPage/aboutPage.js';\nimport Rules from './components/rules/rules.js';\nimport PlayerHub from './components/player-hub/playerHub.js';\nimport store from './store/store.js';\n\n\nfunction App () {\n  const user = useSelector( state => state.user);\n\n\n\n  return (\n    <div className='App'>\n    <Provider store={store}>\n    <BrowserRouter>\n      <div className=\"apps-background\">\n          <Switch>\n            <Route exact path='/' component={Landing}/>\n            <Route exact path='/game' component={Game}/>\n            <Route path=\"/about\" component={About}/>\n            <Route path=\"/rules\" component={Rules}/>\n            <Route path=\"/playerHub\" component={PlayerHub}/>\n          </Switch>\n      </div>\n      </BrowserRouter>\n      </Provider>  \n    </div>\n  );\n};\n\nexport default App;","/home/brend/fluffy-jesters/401-final-react/src/store/gameTableReducer.js",[],"/home/brend/fluffy-jesters/401-final-react/src/store/monsterReducer.js",[],"/home/brend/fluffy-jesters/401-final-react/src/store/treasureReducer.js",[],"/home/brend/fluffy-jesters/401-final-react/src/components/landing-page/landingPage.js",["117"],"import React from 'react';\nimport Footer from '../footer/footer.js';\nimport './landingPage.scss';\nimport { useDispatch} from 'react-redux';\nimport axios from 'axios';\nimport * as actions from '../../store/userReducer.js';\nimport LoggedOutNavbar from '../header/navbar/loggedOutNavbar.js';\n\n\nexport default function Landing() {\n\n\n  const dispatch = useDispatch();\n\n  function loginDispatch(user) {\n    dispatch(actions.loginUser(user));\n  }\n\n\n  const loginData = Object.freeze({\n    username: \"\",\n    password: \"\"\n  });\n\n  const [formData, updateFormData] = React.useState(loginData);\n\n  const handleChange = (e) => {\n    updateFormData({\n      ...formData,\n\n      [e.target.name]: e.target.value.trim()\n    });\n  };\n\n  \n\n  const handleSubmit = async (e) => {\n    e.preventDefault()\n    console.log(formData);\n    try { \n      const resp = await axios({\n      method: 'post',\n      url: 'https://munchkin-auth.herokuapp.com/signin',\n      auth: {\n        username: formData.username,\n        password: formData.password\n      }\n    })\n    if(resp.status === 200) {\n      // console.log(resp.data);\n      localStorage.setItem(\"user login info\",JSON.stringify(resp.data));\n      loginDispatch(resp.data);\n      window.location.href = '/playerHub';\n\n    }\n  } catch (err) {\n      console.log(err);\n  }\n  };\n\n\n  return (\n    <>\n      <LoggedOutNavbar />\n      <div className=\"landing-container\">\n        <div className=\"landing-page\">\n          <label>\n            <input name=\"username\" onChange={handleChange} placeholder=\"Username\" />\n          </label>\n\n          {/* <br /> */}\n\n          <label>\n            <input type=\"password\" name=\"password\" onChange={handleChange} placeholder=\"Password\" id=\"myInput\"/>\n            </label>\n          {/* <br /> */}\n          \n          <button onClick={handleSubmit}>Login</button>\n        </div>\n      </div>\n      {/* <Footer/> */}\n    </>\n  );\n}\n\n","/home/brend/fluffy-jesters/401-final-react/src/components/aboutPage/aboutPage.js",[],"/home/brend/fluffy-jesters/401-final-react/src/components/rules/rules.js",[],"/home/brend/fluffy-jesters/401-final-react/src/components/player-hub/playerHub.js",["118","119","120"],"import { React, useState, useEffect, useRef } from 'react';\nimport { If, Then, Else } from 'react-if';\nimport io from \"socket.io-client\";\nimport LoggedInNavbar from '../header/navbar/loggedInNavbar.js';\nimport Footer from '../footer/footer.js';\nimport './game-table/gameTable.js';\nimport Rooms from './rooms/rooms.js';\nimport CreateRoom from './create-a-room/createRoom.js';\nimport GameTable from './game-table/gameTable.js';\n\nlet id = Math.random(Math.ceil() * 100);\n\nexport default function PlayerHub() {\n    \n  let userD = JSON.parse(localStorage.getItem(\"user login info\"));\n\n  const host = 'https://munchkin-game-center.herokuapp.com/games';\n  // const host = 'http://localhost:3333';\n  \n  const [roomDetail, setRoomDetail] = useState({});\n  const [roomsList, setRoomsList] = useState({});\n  const [localGameState, setGameState] = useState({});\n\n  const socket = useRef();\n\n\n  function joinRoom(room) {\n    socket.current.emit('Join', room);\n    \n    // console.log(room);\n  };\n\n  function createRoom() {\n    socket.current.emit(\"CreateRoom\", userD.user.username);\n    ;\n  };\n\n  function getNewState(state){\n    setGameState(state);\n  };\n\n  function nextTurn(){\n\n    socket.current.emit('nextTurn', localGameState);\n  };\n\n  function updateState() {\n    socket.current.emit(\"updateState\", localGameState);\n  };\n\n  function startGame() {\n    setTimeout(()=>{\n      socket.current.emit('InitGame', {roomOwner: userD.user.username, players:roomDetail.currentPlayers});\n      // let localWinner = undefined;\n    },5000);\n  };\n\n\n \n\n  useEffect(() => {\n    console.log('LocalGameState from playerHub',localGameState);\n\n  }, [localGameState]); \n  useEffect(() => {\n     socket.current = io(host,{\n        query: {\n          user:userD.user.username,\n        }\n      });\n\n      socket.current.on('nextPlayer', (gameState)=>{\n        console.log('next player turn from game center', gameState);\n        setGameState(gameState);\n      });\n\n      socket.current.on('InitialCards', (gameState)=>{\n        setGameState(gameState);\n      \n      });\n\n      socket.current.on('UpdateLocalGameState', (gameState)=> {\n        setGameState(gameState);\n      });\n\n      socket.current.on('NewRoomCreated', (gameRoomInfo)=>{\n        setRoomsList(gameRoomInfo);\n        if(gameRoomInfo[userD.user.username]) {\n          setRoomDetail(gameRoomInfo[userD.user.username]);\n          // console.log(gameRoomInfo)\n        }\n\n      });\n\n      socket.current.on('NewJoin', (payload)=>{\n\n        // console.log('<NewJoin>',payload);\n        \n        setRoomDetail(payload.roomStatus);\n      });\n      socket.current.on('RoomList', (roomList)=>{\n      // console.log('<RoomList>',roomList);\n      if(roomList !== undefined){\n          setRoomsList(roomList);\n      }\n      });\n  }, [userD.user.username, localGameState]);\n\n\n  return (\n    <>\n\n      <If condition={userD.token}>\n        <Then>\n        <LoggedInNavbar/>\n        <div className='playerHub'>\n          {\n            Object.keys(roomDetail).length === 0 &&\n            (\n              <div>\n                <Rooms joinRoom={joinRoom} values={roomsList} className='top'/>\n                <CreateRoom createRoom={createRoom} setRoomDetail={setRoomDetail} className='bottom'/>\n              </div>\n            )\n          }\n            {\n              Object.keys(roomDetail).length !== 0 &&\n                Object.keys(localGameState).length === 0 &&\n\n                (<div>\n                  <ul>\n                    {\n                      \n                        roomDetail.currentPlayers.map((player) => \n                        <div key={id}>\n                          <li>{player.username}</li>\n                        </div>\n                      )\n                    }\n                  </ul>\n                  \n                    <If condition={roomDetail.roomOwner == userD.user.username}>\n                      <Then>\n                        <button onClick={startGame}>Start Game</button>\n                      </Then>\n                    </If>\n          \n                </div>)\n            }\n            {\n              Object.keys(localGameState).length > 0 &&\n                <GameTable newState={getNewState} nextTurn={nextTurn} updateState={updateState} roomDetail={roomDetail} localGameState={localGameState}/>\n            }\n        </div>\n      {/* <Footer/> */}\n        </Then>\n      </If>\n       \n    </>\n  );\n}\n\n","/home/brend/fluffy-jesters/401-final-react/src/components/player-hub/game-table/gameTable.js",["121"],"/home/brend/fluffy-jesters/401-final-react/src/components/footer/footer.js",[],"/home/brend/fluffy-jesters/401-final-react/src/store/userReducer.js",[],"/home/brend/fluffy-jesters/401-final-react/src/components/player-hub/create-a-room/createRoom.js",[],"/home/brend/fluffy-jesters/401-final-react/src/components/player-hub/rooms/rooms.js",[],"/home/brend/fluffy-jesters/401-final-react/src/components/player-hub/game-table/player-progress/playerProgress.js",["122"],"import { pick } from 'lodash';\nimport React from 'react';\nimport './playerProgress.scss';\nlet id = Math.random(Math.ceil() * 100);\n\nfunction PlayerProgress(props) {\n  \n  return (\n    <>\n      <div id=\"\">\n      {\n      props.currentPlayers.map((player) =>\n      <li key={id}>{player.username}</li>\n      ) \n      }\n      </div>\n    </>\n  );\n}\n\nexport default PlayerProgress;\n\n","/home/brend/fluffy-jesters/401-final-react/src/components/player-hub/game-table/active-cards/activeCards.js",["123","124"],"import React, { useState } from 'react';\n// import {useSelector} from 'react-redux';\nimport Card from 'react-bootstrap/Card';\n// import { pick } from 'lodash';\n\nimport './activeCards.scss';\n\n\nexport default function ActiveCards(props) {\n  \n  let userD = JSON.parse(localStorage.getItem(\"user login info\"));\n  const [cardsEquipped, setCardsEquipped] = useState(props.localGameState);\n\n  let equipment = props.localGameState[userD.user.username].cardsEquipped;\n\n  return (\n    <>\n      <div className=\"\">\n      <ul className=\"active-cards-container\" >\n            {\n              equipment.map(card  => <li key={Math.random()}><Card >\n                <Card.Img variant=\"top\" src={card.image} className='equipped-card zoom' />\n                </Card></li>\n            )\n            }\n          </ul> \n      </div>\n    </>\n  );\n};\n\n","/home/brend/fluffy-jesters/401-final-react/src/components/player-hub/game-table/treasure-cards/treasureCards.js",["125"],"/home/brend/fluffy-jesters/401-final-react/src/components/player-hub/game-table/door-cards/doorCards.js",[],"/home/brend/fluffy-jesters/401-final-react/src/components/player-hub/game-table/monster/monster.js",["126","127","128","129","130"],"/home/brend/fluffy-jesters/401-final-react/src/components/player-hub/game-table/cards-in-hand/cardsInHand.js",["131","132"],"import {React,useState} from 'react';\nimport Card from 'react-bootstrap/Card';\nimport { pick } from 'lodash';\nimport { equipCards } from '../../../../game-objects/game-engine.js'; \n\nimport './cardsInHand.scss';\n\n\n\nexport default function CardsInHand(props) {\n\n  let userD = JSON.parse(localStorage.getItem(\"user login info\"));\n  \n  const [activateCard, setActiveCard] = useState(false);\n\n\n  // console.log('player state',playerState);\n\n  \n  let myPlayer = pick(props.localGameState, userD.user.username);\n  console.log('myPlayer', myPlayer);\n  \n  let player = (myPlayer[userD.user.username]);\n  \n\n  let stateUpdater = (cardsInHand, card) => {\n    equipCards(cardsInHand, card);\n    props.newState({...props.localGameState, [userD.user.username] : myPlayer[userD.user.username]}) // [userD.user.username] : myPlayer[userD.user.username]\n    // console.log('inside state updater',playerState);\n     props.updateState();\n  };\n  \n\n  return (\n    <>\n    \n            <ul className=\"cards-in-hand-grid\">\n\n            {\n              player.cardsInHand.map(card => <li onClick